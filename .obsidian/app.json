{
  "trashOption": "local",
  "attachmentFolderPath": "./attachments",
  "strictLineBreaks": true,
  "spellcheck": true,
  "spellcheckLanguages": [
    "zh-CN",
    "en"
  ],
  "alwaysUpdateLinks": true,
  "newLinkFormat": "absolute",
  "showFrontmatter": true,
  "useMarkdownLinks": true,
  "pdfExportSettings": {
    "includeName": false,
    "pageSize": "A4",
    "landscape": false,
    "margin": "0",
    "downscalePercent": 100
  }
}